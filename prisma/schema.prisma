generator client {
  provider = "prisma-client-js"
 }

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  ID_USUARIO Int @id @default(autoincrement())
  NM_USUARIO String
  CD_LOGIN String @unique
  CD_SENHA String
  NR_SALDO Int  @default(0)

  @@map("USUARIO")
}